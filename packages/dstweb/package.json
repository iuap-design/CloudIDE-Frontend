{
  "name": "dst_ui",
  "version": "0.1.0",
  "description": "isomorphic web app for dst ui.",
  "main": "bin/server/index.js",
  "repository": "git@git.yonyou.com:dstweb/dstweb.git",
  "author": "fangqg@yonyou.com",
  "license": "ISC",
  "scripts": {
    "start": "cross-env NODE_ENV=production node bin/server/index.js",
    "debug:client": "cross-env BABEL_ENV=development webpack-dev-server --progress --colors  --config webpack.dev.config.js",
    "debug:server": "cross-env BABEL_ENV=production NODE_ENV=development SRV_URL=http://172.20.17.122:8080 PRINT_SERVER=http://uretailserver.yonyouup.com/print_service nodemon -w src/server -w src/common -w src/yxyweb/server -w src/yxyweb/common --exec babel-node --inspect src/server/app.jsx",
    "build:dll:dev": "cross-env NODE_ENV=development webpack --config webpack.dll.config.js",
    "build:dll:prod": "cross-env NODE_ENV=production webpack --config webpack.dll.config.js",
    "build:dll": "npm run build:dll:dev && npm run build:dll:prod",
    "build:server": "cross-env BABEL_ENV=production babel src -d bin --ignore client,yxyweb/client && echo '后端程序：编译完成'",
    "build:client": "cross-env BABEL_ENV=production webpack --config webpack.prod.config.js --colors --progress && echo '前端程序：编译完成'",
    "build": "npm run build:server && npm run build:client",
    "prebuild": "npm run clean",
    "clean": "rimraf ./bin"
  },
